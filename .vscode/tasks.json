        {
            "label": "Run All Linters",
            "type": "process",
            "command": "pwsh.exe",
            "args": [
                "-c",
                "npm run lint && npm run format:check"
            ],
            "problemMatcher": [
                "$eslint-stylish"
            ],
            "presentation": {
                "reveal": "always",
                "panel": "shared",
                "focus": false,
                "clear": true
            }
        },
        {
            "label": "Lint & Fix",
            "type": "process",
            "command": "pwsh.exe",
