            ],
            "isBackground": true,
            "problemMatcher": [],
            "presentation": {
                "reveal": "always",
                "panel": "dedicated",
                "focus": false,
                "clear": false
            }
        },
        {
            "label": "Full Quality Check",
            "dependsOn": [
                "Run All Linters",
                "Run Tests",
                "Build Project"
            ],
            "dependsOrder": "sequence",
            "problemMatcher": [],
            "presentation": {
                "reveal": "always",
                "panel": "dedicated",
                "focus": true,
                "clear": true
            }
        },
        {
            "label": "Python Setup",
            "type": "process",
            "command": "powershell",
            "args": [
                "-ExecutionPolicy",
                "Bypass",
                "-File",
                "setup-python.ps1"
            ],
            "problemMatcher": [],
            "presentation": {
                "reveal": "always",
                "panel": "shared",
                "focus": false,
                "clear": true
            }
        },
        {
            "label": "Ruff Lint",
            "type": "process",
            "command": "uv",
            "args": [
                "run",
                "--python",
                "3.14t",
                "ruff",
                "check",
                "docker-compose-examples/mcp/python_utils/",
                "--config",
                "docker-compose-examples/mcp/python_utils/pyproject.toml"
            ],
            "problemMatcher": [
                {
                    "owner": "ruff",
                    "source": "ruff",
                    "fileLocation": "relative",
                    "pattern": {
                        "regexp": "^(.+?):(\\d+):(\\d+): (\\w+) (.+)$",
                        "file": 1,
                        "line": 2,
                        "column": 3,
                        "severity": 4,
                        "message": 5
                    }
                }
            ],
            "presentation": {
                "reveal": "always",
                "panel": "shared",
                "focus": false,
                "clear": true
            }
        },
        {
            "label": "Ruff Fix",
            "type": "process",
            "command": "uv",
            "args": [
                "run",
                "--python",
                "3.14t",
                "ruff",
                "check",
                "--fix",
                "docker-compose-examples/mcp/python_utils/",
                "--config",
                "docker-compose-examples/mcp/python_utils/pyproject.toml"
            ],
            "problemMatcher": [
                {
                    "owner": "ruff",
                    "source": "ruff",
                    "fileLocation": "relative",
                    "pattern": {
                        "regexp": "^(.+?):(\\d+):(\\d+): (\\w+) (.+)$",
                        "file": 1,
                        "line": 2,
                        "column": 3,
                        "severity": 4,
                        "message": 5
                    }
                }
            ],
            "presentation": {
                "reveal": "always",
                "panel": "shared",
                "focus": false,
                "clear": true
            }
        },
        {
            "label": "isort Check",
            "type": "process",
            "command": "uv",
            "args": [
                "run",
                "--python",
                "3.14t",
                "isort",
                "--check-only",
                "--diff",
                "--settings-path",
                "docker-compose-examples/mcp/python_utils/pyproject.toml",
                "docker-compose-examples/mcp/python_utils/"
            ],
            "problemMatcher": [],
            "presentation": {
                "reveal": "always",
                "panel": "shared",
                "focus": false,
                "clear": true
            }
        },
        {
            "label": "isort Fix",
