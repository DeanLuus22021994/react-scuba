        },
        {
            "label": "Python Test",
            "type": "process",
            "command": "pwsh.exe",
            "args": [
                "-c",
                "uv run --python 3.14t pytest docker-compose-examples/mcp/python_utils/tests/ -v --tb=short"
            ],
            "problemMatcher": [
                {
                    "owner": "pytest",
                    "source": "pytest",
                    "fileLocation": "relative",
                    "pattern": {
                        "regexp": "^(.+?)::(.+?) (.+?)$",
                        "file": 1,
                        "message": 3
                    }
                }
            ],
            "presentation": {
                "reveal": "always",
                "panel": "shared",
                "focus": false,
                "clear": true
            }
        },
        {
            "label": "Python Coverage",
            "type": "process",
            "command": "pwsh.exe",
            "args": [
                "-c",
                "uv run --python 3.14t pytest docker-compose-examples/mcp/python_utils/tests/ --cov=react_scuba_utils --cov-report=html --cov-report=term-missing"
            ],
            "problemMatcher": [],
            "presentation": {
                "reveal": "always",
                "panel": "dedicated",
                "focus": false,
                "clear": true
            }
        },
        {
            "label": "Python Quality Check",
            "dependsOn": [
