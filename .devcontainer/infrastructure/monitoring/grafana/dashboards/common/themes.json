{
  "style": "dark",
  "theme": {
    "colors": {
      "success": "green",
      "warning": "yellow",
      "critical": "red",
      "info": "blue",
      "neutral": "grey"
    },
    "thresholds": {
      "cpu": {
        "warning": 70,
        "critical": 90
      },
      "memory": {
        "warning": 80,
        "critical": 95
      },
      "disk": {
        "warning": 80,
        "critical": 95
      },
      "database_connections": {
        "warning": 80,
        "critical": 95
      }
    }
  },
  "graphTooltip": 1,
  "liveNow": false,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  },
  "timezone": "browser"
}
